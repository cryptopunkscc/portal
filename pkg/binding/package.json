{
  "name": "portal",
  "version": "0.0.1",
  "description": "Portal bindings for js",
  "type": "module",
  "scripts": {
    "build": "npm run module && npm run backend && npm run wails",
    "module": "rollup -c",
    "backend": "rollup common.js -o out/common/portal.js --f iife -n portal",
    "wails": "rollup wails.js -o out/wails/portal.js --f iife -n portal"
  },
  "author": "yang@cryptopunks.cc",
  "license": "MIT",
  "devDependencies": {
    "rollup": "^4.12.0",
    "rollup-plugin-generate-package-json": "^3.2.0"
  }
}
